<templateSet group="JavaScript">
  <template name="I18N" value="i18n(&quot;$KEY$&quot;){key: &quot;$KEY$&quot;, en: $SELECTION$, entries: {de: undefined}}," description="surround a text for i18n. Prepares a translation entry for cut/paste into i18n.html" toReformat="false" toShortenFQNames="true">
    <variable name="KEY" expression="lowercaseAndDash(String)" defaultValue="$SELECTION$" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>